local gui = Instance.new("ScreenGui")
gui.Parent = game.CoreGui

local frame = Instance.new("Frame")
frame.Parent = gui
frame.Size = UDim2.new(0, 360, 0, 160)
frame.Position = UDim2.new(0.5, -180, 0.5, -80)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true

local title = Instance.new("TextLabel")
title.Parent = frame
title.Size = UDim2.new(1, -20, 0, 35)
title.Position = UDim2.new(0, 10, 0, 10)
title.BackgroundTransparency = 1
title.Text = "This script's beta has officially ended"
title.TextColor3 = Color3.fromRGB(255,255,255)
title.TextSize = 17
title.Font = Enum.Font.GothamBold
title.TextWrapped = true

local info = Instance.new("TextLabel")
info.Parent = frame
info.Size = UDim2.new(1, -20, 0, 35)
info.Position = UDim2.new(0, 10, 0, 45)
info.BackgroundTransparency = 1
info.Text = "Refer to the Discord for more information."
info.TextColor3 = Color3.fromRGB(210,210,210)
info.TextSize = 15
info.Font = Enum.Font.Gotham
info.TextWrapped = true

local button = Instance.new("TextButton")
button.Parent = frame
button.Size = UDim2.new(0, 170, 0, 38)
button.Position = UDim2.new(0.5, -85, 0, 90)
button.BackgroundColor3 = Color3.fromRGB(88, 101, 242)
button.Text = "ðŸ’¬ Copy Invite"
button.TextColor3 = Color3.fromRGB(255,255,255)
button.TextSize = 15
button.Font = Enum.Font.GothamSemibold
button.BorderSizePixel = 0

local label = Instance.new("TextLabel")
label.Parent = frame
label.Size = UDim2.new(1, 0, 0, 20)
label.Position = UDim2.new(0, 0, 1, -22)
label.BackgroundTransparency = 1
label.Text = ""
label.TextColor3 = Color3.fromRGB(0, 255, 140)
label.TextSize = 14
label.Font = Enum.Font.GothamSemibold

local invite = "https://discord.gg/z67gAShpXz"

button.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(invite)
        label.Text = "Copied!"
    else
        label.Text = "Clipboard not available"
    end
end)
